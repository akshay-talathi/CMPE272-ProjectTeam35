<form class="form-horizontal" action="/organizations/<%=id%>" method="POST">
  <fieldset>
    <div id="legend" style="margin-left: 20px;">
      <legend class="">Edit Organization</legend>
    </div>
    <div class="control-group">
      <!-- Organization Name -->
      <label class="control-label"  for="name">Organization Name</label>
      <div class="controls">
        <input type="text" id="name" name="name" value="<%=name%>" class="input-xlarge" required>
      </div>
    </div>
    <div class="control-group">
      <!-- Decription -->
      <label class="control-label"  for="description">Description</label>
      <div class="controls">
        <textarea class="form-control" id="description" name="description" required cols="30" rows="5" placeholder="Description" class="input-xlarge" style="width: 500px;"><%=description%></textarea>
      </div>
    </div>


 
    <div class="control-group">
      <!-- Button -->
      <div class="controls">
        <button class="btn btn-success" value="save" name="Submit" >Save</button>
      	<button type="button" class="btn btn-danger" name="cancel" onClick = "location.href = '/organizations';">Cancel</button>
      </div>
    </div>
  </fieldset>
</form>
