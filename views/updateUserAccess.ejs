                                	<h3>Update User Access</h3>
								</div>
								<br/>
								<br/>
								<br/>
								<br/>
								<br/>
								
							<form method="POST" id="signup-form" class="yform signup-form  city-hidden" action="/postUpdate/<%=data.u_id%>">
							
							<input type = "hidden" value="<%=data.access_id%>" name = "access_id">
							<div class="form-group">
                                <label for="disabledSelect">User Email Id:</label>
                                <select id="disabledSelect" class="form-control">
                                    <option value="<%=data.u_id%>"><%=email_temp%></option>
                                </select>
                            </div>
                            <br/>
                            <%if(allowed_temp){%>
                            <div class="form-group">
                                <label for="disabledSelect">Access allowed?</label>
                                <select id="isallowed" class="form-control" name = "isallowed">
                                    <option value = "1" selected>Yes</option>
                                 	<option value = "0">No</option>
                                </select>
                            </div>
                            <%}else{%>
                            
                            <div class="form-group">
                                <label>Access allowed?</label>
                                <select id="isallowed" class="form-control" name = "isallowed">
                                    <option value = "1">Yes</option>
                                 	<option value = "0" selected>No</option>
                                </select>
                            </div>
                            <%}%>
                            <br/>
                                 <%
						         	var validity = valid_temp.toString();
						        	validity = validity.substring(0, validity.length-42);         
						         %>
						         
						         <label class="placeholder-sub">Valid Upto:</label>
                                 <input type="text" value="<%=validity%>" required="required" placeholder="Click here for access validity" name="validity" id="datepicker">
                                 
                                 
                            </div>
                            <button type="submit" class="btn btn-default">Update</button>
                            </form>
