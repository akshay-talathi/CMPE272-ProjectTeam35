        <div class="container-fluid">
            <div class="row-fluid">
                <!--/span-->
                <div class="span9" id="content">
                     <div class="row-fluid">
                        <!-- block -->
                        <div class="block">
                            <div class="navbar navbar-inner block-header">
                                <div class="muted pull-left">Bootstrap dataTables with Toolbar</div>
                            </div>
                            <div class="block-content collapse in">
                                <div class="span12">                                
                                	<h3>Assign Access</h3>
								</div>
								<br/>
								<br/>
								<br/>
								<br/>
								<br/>
								
							<form method="POST" id="signup-form" class="yform signup-form  city-hidden" action="/postAccess/<%=ap_id%>">
							
							<input type= "hidden" value="<%=ap_id%>" name = "id">
							
							<div class="form-group">
                                <label>User Email Id:</label>
                                <select name="user" id="user">
    							 <option selected = "selected">Select User</option>
                                 <% if(data.length){
                           
                           		for (var i=0; i<data.length;i++) { %>
    							 
                                 <option value="<%=data[i].id%>"><%=data[i].email%></option>
                                 <!--<input type="hidden" value="<%=data[i].id%>" name="user_id">-->
                                 
                                 <% }
            
         							}else{ %>
         							<div>
         								<span>No Element</span>
         							</div>
         							<%}%>
                                 </select>
                            </div>
                            <br/>
						        <div class="form-group"> 
						         <label class="placeholder-sub">Validity</label>
                                 <input type="text" value="" placeholder="Click here for access validity" name="validity" id="datepicker">
                                 </div>   
                            </div>
                            <button type="submit" class="btn btn-default">Grant Access</button>
                            </form>
                        </div>
